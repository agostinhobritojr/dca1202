#include <stdlib.h>
#include <stdio.h>

int* myrealloc(int* old, int oldsize, 
               int newsize){
  int *novo;
  if(oldsize == newsize){
    return old;
  }
  
}

int main(void){
  int *x, n=5, m=10, i;
  x = (int *)malloc(n*sizeof(int));
  
  x = myrealloc(x, n, m);

  for(i = 0; i<n; i++){
    x [i] = i;
  }
  for(i = 0; i<n; i++){
    printf("%2d, ", x[i]);
  }
  printf("\n");
  printf("x antes  = %p\n", x);
  x = (int*) realloc(x, m*sizeof(int));
  printf("x depois = %p\n", x);
  for(i = 0; i<m; i++){
    printf("%2d, ", x[i]);
  }
  printf("\n");
  free(x);
}





